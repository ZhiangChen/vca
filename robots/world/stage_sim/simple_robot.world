# A simple robot for inclusion in other world files

define lidar ranger # define a ranger named lidar
(
    sensor
    (
        range [ 0.0  30.0 ]
        fov 270.25
        samples 1081
    )
    # generic model properties
    color "black"
    size [ 0.05 0.05 0.1 ]
)

define simple_robot position
(
    drive "diff"
    localization "gps"    

    size [ 0.35 0.35 0.25 ]
    origin [ 0 0 0 0 ]
    gui_nose 0
    
    lidar
    (
        pose [ 0 0 -0.1 0 ]
    )
)
